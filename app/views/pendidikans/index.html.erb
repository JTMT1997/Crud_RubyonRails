<p id="notice"><%= notice %></p>

<h1>Pendidikan</h1>
<%= link_to 'Tambah Pendidikan', new_pendidikan_path %>
<table class="responsive-table  green accent-2">
    <thead>
        <tr>
            <th>Pendidikan terakhir</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @pendidikans.each do |pendidikan| %>
        <tr>
            <td><%= pendidikan.pendidikan_terakhir %></td>
            <td><%= link_to 'Show', pendidikan %></td>
            <td><%= link_to 'Edit', edit_pendidikan_path(pendidikan) %></td>
            <td><%= link_to 'Destroy', pendidikan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
    </tbody>
</table>

<br>